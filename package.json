{
  "name": "park-this",
  "version": "1.0.0",
  "description": "Real time parking garage application",
  "main": "index.js",
  "scripts": {
    "docker-init": "docker-compose -f docker-compose.yml -p park_this up --build -d --remove-orphans",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PilotProgrammer/park-this.git"
  },
  "author": "PilotProgrammer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PilotProgrammer/park-this/issues"
  },
  "homepage": "https://github.com/PilotProgrammer/park-this#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "fakerator": "^0.3.3",
    "jest": "^27.0.4",
    "jest-cli": "^27.0.4",
    "source-map-support": "^0.5.19",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3"
  },
  "dependencies": {
    "pg": "^8.6.0",
    "typeorm": "^0.2.34",
    "typescript": "^4.3.4"
  },
  "jest": {
    "automock": false,
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": ".coverage",
    "testRegex": ".*\\.test\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "modeulResolution": "node",
          "module": "commonjs",
          "target": "es6"
        }
      }
    },
    "preset": "ts-jest",
    "testMatch": null
  }
}
